{
  "name": "github-productivity-tracker",
  "displayName": "Github Productivity Tracker",
  "publisher": "RahulPatilDevSapien",
  "description": "This extension allows you to track your code changes and productivity on GitHub.",
  "repository": "https://github.com/rahulpatil8020/github-productivity-tracker",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "github-productivity-tracker.createGithubRepo",
        "title": "Create GitHub Repository"
      },
      {
        "command": "github-productivity-tracker.startTracking",
        "title": "Start Tracking"
      },
      {
        "command": "github-productivity-tracker.stopTracking",
        "title": "Stop Tracking"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.89.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "child-process": "^1.0.2",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "simple-git": "^3.27.0"
  }
}
